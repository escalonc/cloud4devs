{
  "name": "cloud4devs",
  "version": "1.0.0",
  "description": "A multi cloud client for backup your source code",
  "main": "index.js",
  "scripts": {
    "start": "dist/src/index.js",
    "dev": "ts-node src/index.ts",
    "build": "tsc",
    "test": "jest",
    "lint": "eslint src/**/*.ts"
  },
  "jest": {
    "preset": "ts-jest",
    "roots": [
      "<rootDir>/src"
    ],
    "transform": {
      "^.+\\.ts?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.ts?$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json",
      "node"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/escalonc/cloud4devs.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/escalonc/cloud4devs/issues"
  },
  "homepage": "https://github.com/escalonc/cloud4devs#readme",
  "dependencies": {
    "chokidar": "^3.3.0",
    "dropbox": "^4.0.30",
    "fast-glob": "^3.1.1",
    "node-fetch": "^2.6.1",
    "node-find": "^0.6.1"
  },
  "devDependencies": {
    "@types/chokidar": "^2.1.3",
    "@types/glob": "^7.1.1",
    "@types/jest": "^24.0.23",
    "@types/node": "^12.12.17",
    "@types/node-fetch": "^2.5.4",
    "@typescript-eslint/eslint-plugin": "^2.11.0",
    "@typescript-eslint/parser": "^2.11.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "ts-jest": "^24.2.0",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.3"
  }
}
